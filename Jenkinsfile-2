node {
     stage('Clone repository') {
         checkout scm
     }
     stage('Build image') {
         app = docker.build("erp/jenkins")
     }
     stage('Push image') {
         docker.withRegistry('https://harbor.myweb.io', 'harbor') {
             app.push("${env.BUILD_NUMBER}")
             app.push("latest")
         }
     }
}
